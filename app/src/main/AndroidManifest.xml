<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aystech.sandesh">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_sandesh_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_sandesh_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.CorporateRegistrationActivity"></activity>
        <activity android:name=".activity.AddressDetailActivity" />
        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.IndividualRegistrationActivity" />
    </application>

</manifest>